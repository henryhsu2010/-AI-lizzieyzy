a = [10,20, 30, 40, 50, 60]
a2 = [11,13,24,32,48,55]
b = [1,2,3,4,5, 6]

plt.figure(figsize=(6,4))

plt.plot(a,b, 'b--',linewidth=3.5)
plt.plot(a2,b, 'rs',linewidth=2)

plt.title('TEST')
plt.xlabel('XXXXX')
plt.ylabel('YYYYY')
#plt.xlim(30,50)
#plt.ylim(2,4)

#plt.xticks([8,12,22,30,44,58])
plt.grid(True)

plt.show()




